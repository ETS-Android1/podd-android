<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="1dp"
        android:top="-3dp"
        android:left="-3dp"
        android:right="-3dp">
        <shape>
            <stroke
                android:width="2dp"
                android:dashGap="3dp"
                android:dashWidth="3dp"
                android:color="#eee" />

            <solid android:color="#00000000" />
        </shape>
    </item>
</layer-list>